<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="antonio.ejemplos.accesoadatos01" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".PrimeraVentana"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AltaUsuarios"
            android:label="@string/title_activity_alta_usuarios"
            android:parentActivityName=".PrimeraVentana" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="antonio.ejemplos.accesoadatos01.PrimeraVentana" />
        </activity>
        <activity
            android:name=".VerUsuarios"
            android:label="@string/title_activity_ver_usuarios"
            android:parentActivityName=".PrimeraVentana" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="antonio.ejemplos.accesoadatos01.PrimeraVentana" />
        </activity>
        <activity
            android:name=".EditarUsuarios"
            android:label="@string/title_activity_editar_usuarios"
            android:parentActivityName=".PrimeraVentana" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="antonio.ejemplos.accesoadatos01.PrimeraVentana" />
        </activity>
    </application>

</manifest>
